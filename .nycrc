{
    "extension": [
        ".ts"
    ],
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "src/index.ts",
        "**/*.d.ts",
        "node_modules/*",
        "tests/*"
    ],
    "require": [
        "ts-node/register"
    ],
    "reporter": [
        "lcov",
        "html",
        "text"
    ],
    "all": true,
    "check-coverage": false
}
